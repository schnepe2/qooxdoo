{
  "targets": [
    {
      "type": "build",
      "outputPath": "compiled/build",
      "minify": "off",
      "babelOptions": {
        "targets": "node >= 10"
      },
      "deployPath": "lib",
      "writeCompileInfo": true
    }
  ],
  "defaultTarget": "build",
  "locales": [
    "en"
  ],
  "libraries": [
    "."
  ],
  "ignores": [
    "XPathResult",
    "XMLHttpRequest",
    "XMLSerializer",
    "XPathEvaluator",
    "history.*",
    "ActiveXObject",
    "DOMParser",
    "ImageHtml",
    "alert",
    "Image"
  ],
  "applications": [
    {
      "class": "qx.application.Native",
      "name": "qx_server",
      "type": "node",
      "environment": {
        "qx.debug": false
      },
      "include": [
        "qx.core.*",
        "qx.data.*",
        "qx.io.*",
        "qx.lang.*",
        "qx.locale.*",
        "qx.log.*",
        "qx.type.*",
        "qx.util.*",
        "qx.dev.unit.TestLoaderBasic",
        "qx.dev.unit.Sinon"
      ],
      "exclude": [
        "q",
        "qx.test.*",
        "qxWeb",
        "qx.module.*",
        "qx.dev.unit.TestLoader",
        "qx.dev.unit.TestLoaderInline",
        "qx.dev.unit.TestLoaderMobile",
        "qx.dev.unit.TestLoaderNative",
        "qx.ui.*",
        "qx.log.appender.Native",
        "qx.log.appender.Element",
        "qx.bom.Stylesheet",
        "qx.bom.Selector",
        "qx.bom.MediaQuery",
        "qx.bom.Flash",
        "qx.bom.Vml",
        "qx.bom.client.Html",
        "qx.bom.client.Plugin",
        "qx.bom.client.Pdfjs",
        "qx.event.handler.Pointer",
        "qx.event.handler.Gesture",
        "qx.event.handler.PointerCore"
      ]
    }
  ],
  "eslintConfig": {
    "globals": {
      "window": "readonly",
      "performance": "readonly",
      "navigator": "readonly",
      "BigInt": "readonly",
      "Image": "readonly",
      "document": "readonly",
      "Titanium": "readonly",
      "ActiveXObject": "readonly",
      "Packages": "readonly",
      "process": "readonly",
      "require": "readonly",
      "module": "readonly",
      "debug": "readonly",
      "java": "readonly",
      "_dereq_": "readonly"
    },
    "rules": {
      "@qooxdoo/qx/no-refs-in-members": "off",
      "brace-style": "allman",
      "no-warning-comments": "off",
      "no-lonely-if": "off",
      "guard-for-in": "off",
      "max-params": "off",
      "no-nested-ternary": "off",
      "no-console": "off",
      "no-debugger": "off",
      "no-multi-spaces": "off",
      "quotes": "off",
      "no-redeclare": "off",
      "operator-linebreak": "off",
      "no-eq-null": "off",
      "no-useless-call": "off",
      "consistent-return": "off",
      "no-script-url": "off",
      "no-eval": "off",
      "new-cap": "off",
      "no-new": "off",
      "no-unused-vars": "off",
      "max-statements-per-line": "off",
      "no-prototype-builtins": "off",
      "array-callback-return": "off",
      "no-undef-init": "off",
      "no-constant-condition": "off",
      "no-empty": "off",
      "no-control-regex": "off",
      "no-self-compare": "off",
      "no-global-assign": "off",
      "no-fallthrough": "off",
      "no-unreachable": "off",
      "valid-typeof": "off",
      "no-cond-assign": "off",
      "no-else-return": "off",
      "no-unneeded-ternary": "off",
      "one-var": "off",
      "no-extra-semi": "off",
      "no-useless-concat": "off",
      "no-sequences": "off",
      "no-unmodified-loop-condition": "off",
      "no-array-constructor": "off",
      "no-self-assign": "off",
      "no-alert": "off",
      "no-duplicate-case": "off",
      "no-new-object": "off",
      "no-new-wrappers": "off",
      "no-proto": "off",
      "no-extend-native": "off",
      "func-call-spacing": "off",
      "no-unexpected-multiline": "off",
      "no-labels": "off",
      "no-void": "off",
      "no-inner-declarations": "off",
      "no-throw-literal": "off",
      "no-new-wrappers": "off",
      "no-caller": "off",
      "jsdoc/check-alignment": "off",
      "jsdoc/check-examples": "off",
      "jsdoc/check-indentation": "off",
      "jsdoc/check-param-names": "off",
      "jsdoc/check-syntax": "off",
      "jsdoc/check-tag-names": "off",
      "jsdoc/check-types": "off",
      "jsdoc/implements-on-classes": "off",
      "jsdoc/match-description": "off",
      "jsdoc/newline-after-description": "off",
      "jsdoc/no-types": "off",
      "jsdoc/no-undefined-types": "off",
      "jsdoc/require-description": "off",
      "jsdoc/require-description-complete-sentence": "off",
      "jsdoc/require-example": "off",
      "jsdoc/require-hyphen-before-param-description": "off",
      "jsdoc/require-jsdoc": "off",
      "jsdoc/require-param": "off",
      "jsdoc/require-param-description": "off",
      "jsdoc/require-param-name": "off",
      "jsdoc/require-param-type": "off",
      "jsdoc/require-returns": "off",
      "jsdoc/require-returns-check": "off",
      "jsdoc/require-returns-description": "off",
      "jsdoc/require-returns-type": "off",
      "jsdoc/valid-types": "off"
    }
  },
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json"
}
